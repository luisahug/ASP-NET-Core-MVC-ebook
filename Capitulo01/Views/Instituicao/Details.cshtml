@model Modelo.Cadastros.Instituicao

@{
    Layout = "_LayoutIES";
}

@{
    ViewData["Title"] = "Exibir instituição - ";
}

@section Styles {
    <enviroment include="Development">
        <link rel="stylesheet" href="~/lib/datatables/css/jquery.dataTables.min.css" />
        <link rel="stylesheet" href="~/lib/font-awesome/css/all.css" />
    </enviroment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/font-awesome.min.css"
              asp-fallback-href="~/lib/font-awesome/css/all.css"
              asp-fallback-test-class="sr-only" 
              asp-fallback-test-property="position" 
              asp-fallback-test-value="absolute" />
    </environment>
    
     @{await Html.RenderPartialAsync("~/Views/Instituicao/_PartialDetailsContentCard.cshtml", Model); }
}
